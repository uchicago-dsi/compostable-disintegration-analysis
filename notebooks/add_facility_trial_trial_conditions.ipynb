   "metadata": {},
   "outputs": [],
   "source": [
    "def clean_column_names(column):\n",
    "    column = column.lower()\n",
    "    column = column.replace(' ', '_')\n",
    "    column = column.replace('/', '_per_')\n",
    "    column = column.replace(':', '_to_')\n",
    "    for char in ['(', ')', '*', '-', '&', '?', '\\n', '#', '.']:\n",
    "        column = column.replace(char, '')\n",
    "    \n",
    "    # remove leading and trailing underscore\n",
    "    column = column.strip('_')\n",
    "\n",
    "    return column"
   ]
  },
